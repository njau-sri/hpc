<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<TasselPipeline>
    <fork1>
        <importGuess>geno.vcf</importGuess>
    </fork1>
    <fork2>
        <importGuess>pheno.txt</importGuess>
    </fork2>
    <fork3>
        <importGuess>Centered_IBS.txt</importGuess>
    </fork3>
    <combine4>
        <input1/>
        <input2/>
        <intersect/>
    </combine4>
    <combine5>
        <input4/>
        <input3/>
        <mlm>
            <mlmVarCompEst>P3D</mlmVarCompEst>
            <mlmCompressionLevel>None</mlmCompressionLevel>
        </mlm>
        <export>mlm_K_output</export>
    </combine5>
</TasselPipeline>
